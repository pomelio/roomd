import ext.HttpContext as ctx;
import ext.PathRegexp as pregexp;
import std.array as arr;

import hello as hello;

let ppath = ctx.path();

let abr = pregexp.match('/abc/hello', ppath);
if abr {
    let pp = abr['params']['0'];
    pp = arr.join('');
    hello.handle(ctx);
    stop {};
}

throw('not found.');
