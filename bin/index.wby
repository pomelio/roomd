import ext.HttpContext as ctx;
import ext.PathRegexp as pregexp;
import std.array as arr;

let ppath = ctx.path();

let abr = pregexp.match('/abc(.)*', ppath);
if abr {
    let pp = abr['params']['0'];
    pp = arr.slice(pp, 1).join('');
    ctx.dispatch('/' + pp + '.wby', {});
    stop {};
}

throw('not found.');
